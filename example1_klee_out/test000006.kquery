array a_sym[4] : w32 -> w8 = symbolic
array b_sym[4] : w32 -> w8 = symbolic
array c_sym[4] : w32 -> w8 = symbolic
array d_prob_sym[4] : w32 -> w8 = symbolic
(query [(Sle 0
              N0:(ReadLSB w32 0 d_prob_sym))
         (Sle N0 500)
         (Eq false
             (Slt (Add w32 N1:(ReadLSB w32 0 c_sym)
                           N0)
                  (Add w32 (ReadLSB w32 0 a_sym)
                           (ReadLSB w32 0 b_sym))))
         (Eq false (Slt N0 N1))]
        false)
